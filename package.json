{
  "name": "vue-ts-starter",
  "version": "1.0.0",
  "description": "A Vue.js starter boilerplate with typescript",
  "main": "index.js",
  "repository": "ssh://git@www.git.com:10022/wuxueqian/vue-ts-starter.git",
  "author": "wuxueqian <wuxueqian@meituan.com>",
  "license": "MIT",
  "scripts": {
    "dll": "cross-env NODE_ENV=production webpack -p --hide-modules --config build/dll.conf.babel.js",
    "dll:dev": "cross-env NODE_ENV=development webpack --hide-modules --config build/dll.conf.babel.js",
    "build": "rimraf dist && mkdirp dist && cross-env NODE_ENV=production npm run dll && cross-env NODE_ENV=production webpack -p --progress --hide-modules --config build/prod.conf.babel.js",
    "build:dev": "rimraf dist && mkdirp dist && cross-env NODE_ENV=development npm run dll:dev && webpack --progress --hide-modules --config build/dev.conf.babel.js",
    "analyze": "rimraf dist && mkdirp dist && cross-env ANALYZE_ENV=true npm run build",
    "dev": "npm run build:dev && webpack-dev-server --config build/dev.conf.babel.js",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -e node_modules typings -c .tslintrc.json src/**/*.ts",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "tslint --fix -c .tslintrc.json",
    "lint-staged:style": "stylelint --syntax less",
    "format": "prettier --write"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "format",
      "lint-staged:ts",
      "git add"
    ],
    "src/**/*.less": [
      "format",
      "lint-staged:style",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "devDependencies": {
    "@types/node": "^9.4.1",
    "@types/vue": "^2.0.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "customized-progress-webpack-plugin": "^0.0.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "mkdirp": "^0.5.1",
    "node-notifier": "^5.2.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-url": "^7.3.0",
    "rimraf": "^2.6.2",
    "stylelint": "^8.4.0",
    "ts-loader": "^3.4.0",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-property-decorator": "^6.0.0",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
