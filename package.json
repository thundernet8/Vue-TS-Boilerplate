{
  "name": "vue-ts-starter",
  "version": "1.0.0",
  "description": "vue typescript starter boilerplate",
  "main": "index.js",
  "repository": "ssh://git@www.git.com:10022/wuxueqian/vue-ts-starter.git",
  "author": "wuxueqian <wuxueqian@meituan.com>",
  "license": "MIT",
  "scripts": {
    "dll": "cross-env NODE_ENV=production webpack -p --hide-modules --config webpack/dll.conf.babel.js",
    "dll:dev": "cross-env NODE_ENV=development webpack --hide-modules --config webpack/dll.conf.babel.js",
    "build": "rimraf dist && mkdirp dist && cross-env NODE_ENV=production npm run dll && cross-env NODE_ENV=production webpack -p --progress --hide-modules --config webpack/prod.conf.babel.js",
    "build:dev": "rimraf dist && mkdirp dist && cross-env NODE_ENV=development npm run dll:dev && webpack --progress --hide-modules --config webpack/dev.conf.babel.js",
    "analyze": "rimraf dist && mkdirp dist && cross-env ANALYZE_ENV=true npm run build",
    "dev": "npm run build:dev && webpack-dev-server --config webpack/dev.conf.babel.js",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -e node_modules typings -c .tslintrc.json src/**/*.ts",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "tslint --fix -c .tslintrc.json",
    "lint-staged:style": "stylelint --syntax less"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "lint-staged:ts"
    ],
    "src/**/*.less": [
      "lint-staged:style"
    ]
  },
  "pre-commit": "lint-staged",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.3",
    "rimraf": "^2.6.2",
    "ts-loader": "^3.4.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1"
  }
}
