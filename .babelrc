{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                }
            }
        ],
        "es2015",
        "stage-2"
    ],
    "plugins": ["transform-vue-jsx", "transform-runtime"],
    "env": {
        "test": {
            "presets": ["env", "es2015", "stage-2"],
            "plugins": [
                "transform-vue-jsx",
                "transform-es2015-modules-commonjs",
                "dynamic-import-node"
            ]
        }
    }
}
